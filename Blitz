					return {LocalPlayer}
				elseif k:lower() == "random" then
					return game:GetService("Players"):GetPlayers()[math.random(1, #game:GetService("Players"):GetPlayers())]
				else	
					for _, plr in pairs(game:GetService("Players"):GetPlayers()) do

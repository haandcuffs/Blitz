AntiLag.Name = "AntiLag"
AntiLag.Parent = ScrollingFrame
AntiLag.BackgroundColor3 = Color3.fromRGB(233, 53, 62)
AntiLag.BorderSizePixel = 0
AntiLag.Position = UDim2.new(0.231573135, 0, 0.075875476, 0)

				if k:lower() == "all" then
					return game:GetService("Players"):GetPlayers()
				elseif k:lower() == "others" then
					local plrReturn = {}
					for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
						if plr ~= LocalPlayer then
							table.insert(plrReturn, plr)
						end
					end
					return plrReturn

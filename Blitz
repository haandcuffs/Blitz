	end
	local Mouse = game.Players.LocalPlayer:GetMouse()
	local B1Hold = false
	Mouse.Button1Down:Connect(function()
		B1Hold = true 
		while B1Hold == true do
			wait()
			Attack()
		end
	end)
	local Running = false
	Mouse.Button1Up:Connect(function()
		B1Hold = false
	end)
	Mouse.KeyDown:Connect(function(key)
		if key == "z" then
			SpinKick()
		else
			if key == "t" and Attacking == false then
				Attacking = true

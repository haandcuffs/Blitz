local function RHSFN_fake_script() -- heart_2.LocalScript 
	local script = Instance.new('LocalScript', heart_2)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.HomeTab.Visible = false
		script.Parent.Parent.ExtraTab.Visible = false
		script.Parent.Parent.Accessibility.Visible = false
		script.Parent.Parent.FunTab.Visible = true
	end)
end
coroutine.wrap(RHSFN_fake_script)()
